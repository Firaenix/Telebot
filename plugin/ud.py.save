import urbandict

def do(terms): 
	ur = urbandict.define(terms)
	word = ur[1]
	return str(word).replace('{','').replace(r'\n','\n')
    
def help():
	return "!ud string: returns urban dictionary definition of a word"

def getCmd():
	return "!ud"

def getArgs():
	return 1

def hasEncodings():
        return True

